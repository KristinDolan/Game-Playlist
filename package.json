{
  "name": "gameslist",
  "version": "1.0.0",
  "description": "Site to list games played and reviews",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run build-watch\"",
    "server": "nodemon --watch server --ignore server/public server/index.js",
    "seed": "node server/db/seed.js",
    "build": "webpack --config webpack.config.js",
    "build-watch": "webpack --config webpack.config.js --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.7",
    "express": "^4.19.2",
    "pg": "^8.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "babel-loader": "^8.4.1",
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.4",
    "webpack": "^4.47.0",
    "webpack-cli": "^3.3.12"
  }
}
